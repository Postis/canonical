{
    "name": "watson/canonical",
    "description": "Canonical host middleware for Laravel",
    "keywords": ["laravel", "canonical", "redirector"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studiousapp.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/http": ">=5.5"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\Canonical\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Canonical\\CanonicalServiceProvider"
            ],
        }
    },
}
